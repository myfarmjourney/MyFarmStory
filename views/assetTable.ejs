        <table class="container">
                <thead>
                    <tr>
                        <th><h1>No</h1></th>
                        <th><h1>Item Name</h1></th>
                        <th><h1>Category</h1></th>
                        <th><h1>Price</h1></th>
                        <th><h1>Option</h1></th>
                    </tr>
                </thead>

                <tbody>
                        <%datas.forEach(data => { %>
                            <tr>
                                <th scope="row"><%= data.id%></th>
                                <td><%= data.itemName%></td>
                                <td><%= data.kategori%></td>
                                <td><%= data.jual%></td>
                                <td><a href="/assets/<%=data.id%>/sell" class="btn btn-primary">Sell</a></td>
                            </tr>
                        <% });%>
